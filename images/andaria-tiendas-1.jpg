<svg width="1200" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Fondo gris del piso -->
  <rect width="100%" height="100%" fill="#E8E8E8"/>
  
  <!-- Edificio principal con tienda -->
  <rect x="200" y="100" width="800" height="300" fill="#4CAF50"/>
  <text x="600" y="200" dominant-baseline="middle" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" fill="white" font-weight="bold">Cuidado del Perro</text>
  
  <!-- Secciones coloridas -->
  <rect x="0" y="100" width="200" height="300" fill="#1976D2"/>
  <rect x="1000" y="100" width="200" height="300" fill="#FF6B35"/>
  
  <!-- Líneas decorativas blancas -->
  <rect x="200" y="120" width="800" height="8" fill="white"/>
  <rect x="200" y="160" width="800" height="8" fill="white"/>
  <rect x="200" y="200" width="800" height="8" fill="white"/>
  
  <!-- Elementos decorativos coloridos en el suelo -->
  <rect x="100" y="450" width="80" height="60" fill="#FFC107"/>
  <rect x="200" y="470" width="60" height="40" fill="#FF6B35"/>
  <rect x="300" y="460" width="50" height="50" fill="#4CAF50"/>
  <rect x="400" y="450" width="70" height="60" fill="#1976D2"/>
  
  <!-- Plantas/jardín -->
  <ellipse cx="800" cy="500" rx="60" ry="30" fill="#228B22"/>
  <ellipse cx="900" cy="480" rx="40" ry="20" fill="#228B22"/>
  
  <!-- Techo -->
  <rect x="0" y="80" width="1200" height="20" fill="#CCCCCC"/>
  
  <!-- Texto -->
  <text x="50%" y="550" dominant-baseline="middle" text-anchor="middle" font-family="Arial, sans-serif" font-size="28" fill="#333" font-weight="bold">Zona Comercial Interior</text>
</svg>