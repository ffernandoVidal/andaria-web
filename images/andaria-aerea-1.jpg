<svg width="1200" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo cielo -->
  <rect width="100%" height="300" fill="url(#bg1)"/>
  
  <!-- Edificio central con colores vibrantes -->
  <rect x="300" y="150" width="600" height="450" fill="#F0F0F0"/>
  
  <!-- Secciones coloridas del edificio -->
  <rect x="100" y="200" width="150" height="400" fill="#1976D2"/>
  <rect x="250" y="180" width="120" height="420" fill="#4CAF50"/>
  <rect x="950" y="220" width="150" height="380" fill="#FF6B35"/>
  <rect x="1100" y="200" width="100" height="400" fill="#FFC107"/>
  
  <!-- Área central con jardín -->
  <ellipse cx="600" cy="450" rx="200" ry="80" fill="#4CAF50" opacity="0.6"/>
  <circle cx="600" cy="420" r="15" fill="#228B22"/>
  
  <!-- Líneas rojas decorativas -->
  <rect x="370" y="150" width="4" height="200" fill="#FF0000"/>
  <rect x="420" y="150" width="4" height="200" fill="#FF0000"/>
  <rect x="470" y="150" width="4" height="200" fill="#FF0000"/>
  <rect x="520" y="150" width="4" height="200" fill="#FF0000"/>
  <rect x="570" y="150" width="4" height="200" fill="#FF0000"/>
  
  <!-- Montañas de fondo -->
  <polygon points="0,250 200,100 400,200 600,80 800,150 1000,120 1200,180 1200,250" fill="#228B22" opacity="0.3"/>
  
  <!-- Texto -->
  <text x="50%" y="500" dominant-baseline="middle" text-anchor="middle" font-family="Arial, sans-serif" font-size="36" fill="white" font-weight="bold">Vista Aérea Principal</text>
</svg>